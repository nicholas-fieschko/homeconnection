= form_for @blacklisting do |f|
  - if @blacklisting.errors.any?
    #error_explanation
      %h2= "#{pluralize(@blacklisting.errors.count, "error")} prohibited this blacklisting from being saved:"
      %ul
        - @blacklisting.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :blocked_user
    = f.number_field :blocked_user
  .actions
    = f.submit 'Save'
